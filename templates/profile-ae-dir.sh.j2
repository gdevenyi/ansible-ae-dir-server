# {{ ansible_managed }}
# Do not edit this file! It will be overwritten!

LDAPCONF={{ aedir_etc }}/ldap.conf
export LDAPCONF

PATH={{ aedir_bin }}:{{ aedir_sbin }}:$PATH
export PATH

{% if openldap_path.bin != '/usr/bin' %}
PATH={{ openldap_path.bin }}:$PATH
export PATH
{% endif %}

{% if openldap_path.sbin != '/usr/sbin' %}
PATH={{ openldap_path.sbin }}:$PATH
export PATH
{% endif %}
